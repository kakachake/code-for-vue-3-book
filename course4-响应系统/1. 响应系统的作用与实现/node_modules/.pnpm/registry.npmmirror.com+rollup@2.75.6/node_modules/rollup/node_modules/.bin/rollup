#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/frontEnd/code-for-vue-3-book/course4-响应系统/1. 响应系统的作用与实现/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/frontEnd/code-for-vue-3-book/course4-响应系统/1. 响应系统的作用与实现/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../dist/bin/rollup" "$@"
fi
